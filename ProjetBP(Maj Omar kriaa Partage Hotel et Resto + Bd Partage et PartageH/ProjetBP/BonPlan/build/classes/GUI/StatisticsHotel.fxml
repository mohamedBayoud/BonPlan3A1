            <Font size="24.0" />
         </font>
      </Button>
      <BarChart fx:id="BarChart" alternativeColumnFillVisible="true" layoutX="229.0" layoutY="204.0" onDragDetected="#LoadChart" prefHeight="476.0" prefWidth="830.0">
         <xAxis>
            <CategoryAxis side="BOTTOM" />
         </xAxis>
         <yAxis>
            <NumberAxis side="LEFT" />
         </yAxis>
      </BarChart>
      <Label layoutX="232.0" layoutY="37.0" text="Statistiques des Personnes qui ont commenté a une date donnée">
