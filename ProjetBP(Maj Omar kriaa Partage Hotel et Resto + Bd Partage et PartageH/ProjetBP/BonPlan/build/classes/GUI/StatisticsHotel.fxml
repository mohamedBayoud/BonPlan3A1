<?import javafx.scene.layout.*?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.40" fx:controller="GUI.StatisticsHotelController">
   <children>
      <ImageView fitHeight="682.0" fitWidth="1266.0" layoutX="1.0" layoutY="2.0" opacity="0.17" pickOnBounds="true">
         <image>
            <Image url="@HD.jpg" />
         </image>
      </ImageView>
      <Button fx:id="btnLoad" layoutX="572.0" layoutY="111.0" mnemonicParsing="false" onAction="#LoadChart" prefHeight="74.0" prefWidth="144.0" text="Load">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="1134.0" layoutY="35.0" mnemonicParsing="false" onAction="#ReturnButton" prefHeight="51.0" prefWidth="108.0" text="Retour">
